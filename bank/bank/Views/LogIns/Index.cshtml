@model IEnumerable<bank.Models.LogIn>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Paswrd)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Paswrd)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.OId }) |
            @Html.ActionLink("Details", "Details", new { id = item.OId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.OId })
        </td>
    </tr>
}

</table>
